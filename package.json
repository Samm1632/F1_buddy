{
  "name": "usa-visa-buddy",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "USA Visa Buddy - MEVN app providing visa guidance with RAG",
  "scripts": {
    "dev:server": "nodemon backend/server.js",
    "dev:client": "vite --config frontend/vite.config.js",
    "dev": "concurrently -k -n server,client -c blue,green \"npm:dev:server\" \"npm:dev:client\"",
    "seed": "node backend/scripts/seed.js",
    "build": "vite build --config frontend/vite.config.js",
    "preview": "vite preview --config frontend/vite.config.js",
    "start": "node backend/server.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.5.3",
    "openai": "^4.56.0",
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "mongodb-memory-server": "^10.1.4",
    "vite": "^5.4.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
